<jboss-deployment-structure
	xmlns="urn:jboss:deployment-structure:1.2">

	<deployment>
		<dependencies>
			<module name="deployment.yonoos" annotations="true"
				export="true" meta-inf="export" optional="false" services="import"
				slot="main" />
		</dependencies>

	</deployment>

	<sub-deployment name="jee-test-web.war">
		<!-- This corresponds to the module for a web deployment -->
		<!-- it can use all the same tags as the <deployment> entry above -->
		<dependencies>
			<!-- Adds a dependency on a ejb jar. This could also be done with a Class-Path 
				entry -->
			<module name="deployment.yonoos" annotations="true"
				export="true" meta-inf="export" optional="false" services="import"
				slot="main" />
		</dependencies>
	</sub-deployment>

	<module name="deployment.yonoos" slot="main">
		<resources>
			<resource-root
				path="C:\WEXLOG\dev\sources\perso\ws-test\ws-test\younes.module\target\classes" />
		</resources>
	</module>

</jboss-deployment-structure>